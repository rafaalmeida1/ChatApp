{
    "version": 2,
    "builds": [
      {
        "src": "api/websocket.js",
        "use": "@vercel/node"
      },
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      { "src": "/api/websocket", "dest": "api/websocket.js" },
      { "src": "/(.*)", "dest": "next.config.js" }
    ]
  }
  